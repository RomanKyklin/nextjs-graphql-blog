{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "knex": "node -r esm node_modules/.bin/knex",
    "db:repl": "babel-node scripts/db-repl.js",
    "db:generate": "babel-node scripts/db-generate.js",
    "db:make": "yarn knex migrate:make",
    "db:up": "yarn knex migrate:up",
    "db:down": "yarn knex migrate:down",
    "db:rollback": "yarn knex migrate:rollback",
    "db:rollbackAll": "yarn knex migrate:rollback --all",
    "db:latest": "yarn knex migrate:latest",
    "db:reset": "yarn db:rollbackAll && yarn db:latest"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-server-core": "latest",
    "apollo-server-micro": "^2.12.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "fs": "^0.0.1-security",
    "gray-matter": "^4.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.0",
    "micro-cors": "^0.1.1",
    "next": "9.3.5",
    "next-runtime-dotenv": "^1.2.0",
    "next-with-apollo": "^5.0.1",
    "node-fetch": "latest",
    "pg": "^8.0.3",
    "prop-types": "latest",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "graphql": "latest",
    "graphql-tools": "latest"
  }
}
